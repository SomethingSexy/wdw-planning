{
  "name": "wdw-planning",
  "version": "0.1.0",
  "private": true,
  "description": "WDW vacation planning application.",
  "main": "modules/server/index.js",
  "scripts": {
    "build:client": "webpack",
    "build:server": "tsc -p server.tsconfig.json",
    "postversion": "git push && git push --tags",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./lib/server/server/index.js",
    "start:core": "node -r dotenv/config ./lib/server/server/index.js",
    "start:dev": "npm run build:client && npm run build:server && npm run start:core",
    "test": "mocha --opts test/mocha.opts",
    "version": "npm run build && git add -A modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://somethingsexy@github.com/SomethingSexy/wdw-planning.git"
