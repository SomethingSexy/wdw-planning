  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "bloomer": "^0.6.3",
    "bulma": "^0.6.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "joi": "^14.0.0",
    "mobx": "^5.5.0",
    "mobx-react": "^5.3.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.2.2",
