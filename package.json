{
  "name": "wdw-planning",
  "version": "0.1.0",
  "private": true,
  "description": "WDW vacation planning application.",
  "main": "modules/server/index.js",
  "scripts": {
    "build:clean": "rimraf ./lib",
    "build:client": "parcel build --public-url public --out-dir ./lib/public ./src/client/index.html",
    "build:server": "tsc -p server.tsconfig.json && copyfiles -u 1 ./src/**/*.css ./lib/server/",
    "postversion": "git push && git push --tags",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./lib/server/server/index.js",
    "start:core": "node -r dotenv/config ./lib/server/server/index.js",
    "start:dev": "cross-env NODE_ENV=development npm run build:clean && npm run build:client && npm run build:server && npm run start:core",
    "test": "mocha --opts test/mocha.opts",
    "version": "npm run build && git add -A modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://somethingsexy@github.com/SomethingSexy/wdw-planning.git"
  },
  "author": "Tyler Cvetan",
  "bugs": {
    "url": "https://github.com/SomethingSexy/wdw-planning/issues"
  },
  "homepage": "https://github.com/SomethingSexy/wdw-planning#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "create-react-class": "^15.6.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "fetch-everywhere": "^1.0.5",
    "ignore-styles": "^5.0.1",
    "joi": "^14.0.0",
    "mobx": "^5.5.0",
    "mobx-react": "^5.3.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-config": "^4.4.0-beta.1",
    "react-router-dom": "^4.2.2",
    "react-router-transition": "^1.2.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "semantic-ui-react": "^0.83.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/create-react-class": "^15.6.2",
    "@types/joi": "^13.6.1",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-config": "^1.0.9",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.5.4",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "parcel-bundler": "^1.10.3",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  }
}
